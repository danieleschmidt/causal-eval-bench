{
  "branches": [
    "main",
    {
      "name": "develop",
      "prerelease": "beta"
    },
    {
      "name": "alpha",
      "prerelease": "alpha"
    }
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits",
        "releaseRules": [
          {
            "type": "feat",
            "release": "minor"
          },
          {
            "type": "fix",
            "release": "patch"
          },
          {
            "type": "perf",
            "release": "patch"
          },
          {
            "type": "docs",
            "release": "patch"
          },
          {
            "type": "style",
            "release": false
          },
          {
            "type": "refactor",
            "release": "patch"
          },
          {
            "type": "test",
            "release": false
          },
          {
            "type": "ci",
            "release": false
          },
          {
            "type": "chore",
            "release": false
          },
          {
            "type": "build",
            "release": "patch"
          },
          {
            "scope": "breaking",
            "release": "major"
          }
        ]
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "presetConfig": {
          "types": [
            {
              "type": "feat",
              "section": "âœ¨ Features",
              "hidden": false
            },
            {
              "type": "fix",
              "section": "ðŸ› Bug Fixes",
              "hidden": false
            },
            {
              "type": "perf",
              "section": "âš¡ Performance Improvements",
              "hidden": false
            },
            {
              "type": "docs",
              "section": "ðŸ“š Documentation",
              "hidden": false
            },
            {
              "type": "refactor",
              "section": "â™»ï¸ Code Refactoring",
              "hidden": false
            },
            {
              "type": "build",
              "section": "ðŸ—ï¸ Build System",
              "hidden": false
            },
            {
              "type": "ci",
              "section": "ðŸ‘· CI/CD",
              "hidden": true
            },
            {
              "type": "test",
              "section": "ðŸ§ª Tests",
              "hidden": true
            },
            {
              "type": "style",
              "section": "ðŸ’„ Styling",
              "hidden": true
            },
            {
              "type": "chore",
              "section": "ðŸ”§ Maintenance",
              "hidden": true
            }
          ]
        }
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md",
        "changelogTitle": "# Changelog\n\nAll notable changes to this project will be documented in this file.\n\nThe format is based on [Keep a Changelog](https://keepachangelog.com/en/1.0.0/),\nand this project adheres to [Semantic Versioning](https://semver.org/spec/v2.0.0.html)."
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "poetry version ${nextRelease.version}",
        "publishCmd": "poetry build && poetry publish"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "CHANGELOG.md",
          "pyproject.toml"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "dist/*.tar.gz",
            "label": "Source distribution"
          },
          {
            "path": "dist/*.whl",
            "label": "Python wheel"
          }
        ],
        "addReleases": "bottom",
        "successComment": "ðŸŽ‰ This ${issue.pull_request ? 'PR is included' : 'issue has been resolved'} in version [${nextRelease.version}](${releases.filter(release => release.name === nextRelease.gitTag)[0].url}) ðŸŽ‰",
        "failTitle": "âŒ Release failed",
        "failComment": "The release from branch `${branch.name}` failed due to the following errors:\n\n${errors.map(err => err.message).join('\\n\\n')}"
      }
    ]
  ],
  "preset": "conventionalcommits",
  "tagFormat": "v${version}",
  "repositoryUrl": "https://github.com/your-org/causal-eval-bench.git",
  "debug": false,
  "ci": true,
  "dryRun": false
}